<h1>Questions</h1>

<%= @questions.inspect %>
<h1>each</h1>
<table  width="300" bgcolor="plum" border="1">
    <tbody>
        <% @questions.each do |question| %>
            <tr>
                <td><%= question.id %></td>
                <td><%= question.title %></td>
                <td><%= question.body %></td>
                <td><time datetime="<%= question.formated_created_at %>"><%= question.formated_created_at %></time></td>
            </tr>
        <% end %>
    </tbody>
</table>